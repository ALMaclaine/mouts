{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@mouts/array": ["packages/array/src/index.ts"],
      "@mouts/constants": ["packages/constants/src/index.ts"],
      "@mouts/core": ["packages/core/src/index.ts"],
      "@mouts/function": ["packages/function/src/index.ts"],
      "@mouts/is": ["packages/is/src/index.ts"],
      "@mouts/math": ["packages/math/src/index.ts"],
      "@mouts/number": ["packages/number/src/index.ts"],
      "@mouts/object": ["packages/object/src/index.ts"],
      "@mouts/types": ["packages/types/src/index.ts"],
      "mouts": ["packages/mouts/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
